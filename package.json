{
  "name": "dall-clock",
  "displayName": "DALLÂ·clock",
  "publisher": "roblourens",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dall-clock.refresh",
        "title": "Refresh",
        "icon": "$(sync)"
      },
      {
        "command": "dall-clock.refresh-dummy",
        "title": "Refresh",
        "icon": "$(sync~spin)",
        "enablement": "false"
      },
      {
        "command": "dall-clock.open",
        "title": "Open full picture"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dall-clock.refresh",
          "when": "view == dall-clock && !dall-clock.refreshing",
          "group": "navigation"
        },
        {
          "command": "dall-clock.refresh-dummy",
          "when": "view == dall-clock && dall-clock.refreshing",
          "group": "navigation"
        },
        {
          "command": "dall-clock.open",
          "when": "view == dall-clock"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "dall-clock",
          "name": "Dall-Clock"
        }
      ]
    },
    "configuration": {
      "properties": {
        "dall-clock.location": {
          "type": "string",
          "description": "A free-form description of your location, like a city name.",
          "default": "Seattle, WA"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "openai": "^4.20.0"
  }
}
